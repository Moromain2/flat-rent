<%= form_for @proprety do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>

  <%= f.label :image %>
  <%= f.file_field :image %>
  <% if @proprety.image.present? %>
    <%= image_tag(@proprety.image_url(:thumb).to_s) %>
  <% end %>
  <br>

  <%= f.label :image2 %>
  <%= f.file_field :image2 %>
  <% if @proprety.image2.present? %>
    <%= image_tag(@proprety.image2_url(:thumb).to_s) %>
  <% end %>
  <br>

  <%= f.label :image3 %>
  <%= f.file_field :image3 %>
  <% if @proprety.image3.present? %>
    <%= image_tag(@proprety.image3_url(:thumb).to_s) %>
  <% end %>
  <br>

  <%= f.label :image4 %>
  <%= f.file_field :image4 %>
  <% if @proprety.image4.present? %>
    <%= image_tag(@proprety.image4_url(:thumb).to_s) %>
  <% end %>
  <br>

  <%= f.label :image5 %>
  <%= f.file_field :image5 %>
  <% if @proprety.image5.present? %>
    <%= image_tag(@proprety.image5_url(:thumb).to_s) %>
  <% end %>
  <br>

  <%= f.label :image6 %>
  <%= f.file_field :image6 %>
  <% if @proprety.image6.present? %>
    <%= image_tag(@proprety.image6_url(:thumb).to_s) %>
  <% end %>
  <br>

  <%= f.label :image7 %>
  <%= f.file_field :image7 %>
  <% if @proprety.image7.present? %>
    <%= image_tag(@proprety.image7_url(:thumb).to_s) %>
  <% end %>
  <br>

  <%= f.label :description %>
  <%= f.text_area :description %>
  <br>

  <%= f.label :people %>
  <%= f.number_field :people %>
  <br>

  <%= f.label :bedrooms %>
  <%= f.number_field :bedrooms %>
  <br>

  <%= f.label :bathrooms %>
  <%= f.number_field :bathrooms %>
  <br>

  <%= f.label :wifi %>
  <%= f.check_box :wifi %>
  <br>

  <%= f.label :parking %>
  <%= f.check_box :parking %>
  <br>

  <%= f.label :lift %>
  <%= f.check_box :lift %>
  <br>

  <%= f.label :kitchen %>
  <%= f.number_field :kitchen %>
  <br>

  <%= f.label :other %>
  <%= f.text_field :other %>
  <br>

  <%= f.label :price %>
  <%= f.number_field :price %>
  <br>

  <%= f.label :street %>
  <%= f.text_field :street %>
  <br>

  <%= f.label :number %>
  <%= f.number_field :number %>
  <br>

  <%= f.label :zipcode %>
  <%= f.number_field :zipcode %>
  <br>

  <%= f.label :city %>
  <%= f.text_field :city %>
  <br>

  <%= f.label :type_of_street %>
  <%= f.text_field :type_of_street %>
  <br>

  <%= f.submit 'Enregistrer' %>

<% end %>
